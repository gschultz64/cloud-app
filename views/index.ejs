<div class="row">
  <div class="col s12">
    <h1>Cloud App home page</h1>
  </div>
  <div class="col s12">
    <% arr.forEach(function(item, i) { %>
    <% var imgUrl = cloud.url(item, {width: 100, height: 150}) %>
    <div class="col s3">
      <a href="<%= imgUrl %>">Image <%= i + 1 %><img src="<%= imgUrl %>"</a>
    </div>
    <% }); %>
  </div>
</div>